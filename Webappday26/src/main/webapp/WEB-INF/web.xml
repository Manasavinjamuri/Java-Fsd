<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
<display-name>Practice1</display-name>

  
  <servlet>
 
    <servlet-name>Factorial</servlet-name>
    <servlet-class>aa.jobiak.webapp.Factorial</servlet-class>
 </servlet>
  <servlet-mapping>
	<servlet-name>Factorial</servlet-name>
	<url-pattern>/begins</url-pattern>
</servlet-mapping>


<servlet>
 
    <servlet-name>JdbcServlet</servlet-name>
    <servlet-class>aa.jobiak.webapp.JdbcServlet</servlet-class>
 </servlet>
  <servlet-mapping>
	<servlet-name>JdbcServlet</servlet-name>
	<url-pattern>/connect</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>LifeCycleServlet</servlet-name>
    <servlet-class>aa.jobiak.webapp.LifeCycleServlet</servlet-class>
 </servlet>
  <servlet-mapping>
	<servlet-name>LifeCycleServlet</servlet-name>
	<url-pattern>/go</url-pattern>
</servlet-mapping>


<welcome-file-list>
<welcome-file>links.html</welcome-file>
</welcome-file-list>

</web-app>