<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Webappday25</display-name>
  <servlet>
    <servlet-name>Factorial</servlet-name>
    <servlet-class>aa.jobiak.webapp.Factorial</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Factorial</servlet-name>
    <url-pattern>/begins</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JdbcServlet</servlet-name>
    <servlet-class>aa.jobiak.webapp.JdbcServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JdbcServlet</servlet-name>
    <url-pattern>/connect</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LifeCycleServlet</servlet-name>
    <servlet-class>aa.jobiak.webapp.LifeCycleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LifeCycleServlet</servlet-name>
    <url-pattern>/go</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>links.html</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
    
    <display-name>JdbcConfigServlet</display-name>
    <servlet-name>JdbcConfigServlet</servlet-name>
    <servlet-class>ai.jobiak.http.JdbcConfigServlet</servlet-class>
    <init-param>
      <description>username for db</description>
      <param-name>username</param-name>
      <param-value>root</param-value>
    </init-param>
    <init-param>
      <description>db password</description>
      <param-name>password</param-name>
      <param-value>admin</param-value>
    </init-param>
    <init-param>
      <description>mysqlconnection</description>
      <param-name>url</param-name>
      <param-value>jdbc:mysql://localhost:3306/world</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>JdbcConfigServlet</servlet-name>
    <url-pattern>/JdbcConfigServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>a</display-name>
    <servlet-name>a</servlet-name>
    <servlet-class>ai.jobiak.http.A</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>a</servlet-name>
    <url-pattern>/a</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>b</display-name>
    <servlet-name>b</servlet-name>
    <servlet-class>ai.jobiak.http.B</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>b</servlet-name>
    <url-pattern>/b</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>C</display-name>
    <servlet-name>C</servlet-name>
    <servlet-class>ai.jobiak.http.C</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>C</servlet-name>
    <url-pattern>/c</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DataBaseServlet</display-name>
    <servlet-name>DataBaseServlet</servlet-name>
    <servlet-class>ai.jobiak.http.DataBaseServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DataBaseServlet</servlet-name>
    <url-pattern>/DataBaseServlet</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
  <welcome-file>
  links.html
  </welcome-file>
  </welcome-file-list>
</web-app>